{
  "title": "操作手册",
  "author": "xushujin",
  "description": "我的操作手册",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./static/styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {
    "sidebar": {
      "我的主页": "https://xushujin.gitee.io/homepage"
    }
  },
  "plugins": [
    "-sharing",
    "splitter",
    "expandable-chapters-small",
    "anchors",
    "github",
    "github-buttons",
    "donate",
    "sharing-plus",
    "anchor-navigation-ex",
    "favicon"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/xushujin"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "xushujin",
          "repo": "gitboot",
          "type": "star",
          "size": "small",
          "count": true
        }
      ]
    },
    "donate": {
      "alipay": "./static/images/wechat_pay.jpg",
      "title": "",
      "button": "赞赏",
      "alipayText": " "
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "google",
        "facebook",
        "twitter",
        "weibo",
        "qq",
        "qzone"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "favicon": {
      "shortcut": "./static/images/favicon.jpg",
      "bookmark": "./static/images/favicon.jpg"
    }
  }
}
